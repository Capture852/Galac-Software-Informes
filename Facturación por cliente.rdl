FROM dbo.factura
INNER JOIN dbo.Cliente
ON factura.ConsecutivoCompania = Cliente.ConsecutivoCompania
AND factura.CodigoCliente = Cliente.Codigo

INNER JOIN Comun.cambio 
ON Factura.Fecha = Comun.Cambio.FechaDeVigencia
AND Comun.cambio.CodigoMoneda='USD'

