    <DataSet Name="dsSeleccionarCliente">
      <Query>
        <DataSourceName>DsSaw</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FiltroCliente">
            <Value>=Parameters!FiltroCliente.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ConsecutivoCompania">
            <Value>=Parameters!ConsecutivoCompania.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Codigo, Nombre
FROM dbo.Cliente
WHERE Nombre LIKE '%'+@FiltroCliente +'%'
AND ConsecutivoCompania = @ConsecutivoCompania
