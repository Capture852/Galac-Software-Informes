    <DataSet Name="dsQuery">
      <Query>
        <DataSourceName>DsSaw</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FiltroFechaDesde">
            <Value>=Parameters!FiltroFechaDesde.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FiltroFechaHasta">
            <Value>=Parameters!FiltroFechaHasta.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ConsecutivoCompania">
            <Value>=Parameters!ConsecutivoCompania.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CodigoCliente">
            <Value>=Parameters!CodigoCliente.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET DATEFORMAT DMY
SELECT
/* ,COUNT(dbo.factura.Numero) AS [Cantidad Facturas] */
/* ,SUM(ROUND(CAST(dbo.factura.TotalFactura / Comun.Cambio.CambioAMonedaLocal AS DECIMAL(20,2)), 2)) AS [Total Dólares] */

 MAX(dbo.factura.Numero) AS [Nro.Factura]
 ,dbo.cliente.Nombre AS [Nombre del Cliente]

 ,factura.Moneda AS [MonedaDocumento]

 ,dbo.factura.TotalRenglones AS [Total Base Imponible]
 ,dbo.factura.TotalIVA AS [Total Iva]
 ,dbo.factura.TotalFactura AS [Total Factura]
 ,dbo.factura.MontoDescuento1 AS [Total Descuento]

 ,FORMAT(CONVERT(DATETIME, MAX(dbo.factura.Fecha), 102),'dd/MM/yyyy ') AS [Fecha]
 ,Comun.Cambio.CambioAMonedaLocal AS [Tasa del Día]

FROM dbo.factura
INNER JOIN dbo.Cliente
ON factura.ConsecutivoCompania = Cliente.ConsecutivoCompania
AND factura.CodigoCliente = Cliente.Codigo

INNER JOIN Comun.cambio 
ON Factura.Fecha = Comun.Cambio.FechaDeVigencia
AND Comun.cambio.CodigoMoneda='USD'

WHERE (dbo.factura.Fecha BETWEEN @FiltroFechaDesde AND @FiltroFechaHasta) 
AND dbo.factura.ConsecutivoCompania = @ConsecutivoCompania
AND (dbo.factura.StatusFactura = '0')
AND (dbo.Factura.TipoDeDocumento = '0')
AND dbo.Cliente.Codigo LIKE ( CASE WHEN @CodigoCliente = '0' THEN '%' ELSE @CodigoCliente END)
GROUP BY factura.CodigoCliente
        ,Cliente.Nombre
	,factura.Moneda
	,factura.Numero
	,comun.cambio.CambioAMonedaLocal
	,dbo.factura.TotalRenglones
	,dbo.factura.TotalIVA
	,dbo.factura.TotalFactura
	,dbo.factura.MontoDescuento1
ORDER BY dbo.factura.Numero</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Nro_Factura">
          <DataField>Nro.Factura</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Nombre_del_Cliente">
          <DataField>Nombre del Cliente</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MonedaDocumento">
          <DataField>MonedaDocumento</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_Base_Imponible">
          <DataField>Total Base Imponible</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Total_Descuento">
          <DataField>Total Descuento</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Total_Iva">
          <DataField>Total Iva</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Total_Factura">
          <DataField>Total Factura</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Fecha">
          <DataField>Fecha</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Tasa_del_Día">
          <DataField>Tasa del Día</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalBaseImponibleAlCambio">
          <Value>=
IIf(Parameters!FiltroMoneda.Value="Bolívar"And Fields!MonedaDocumento.Value="Dólar",
      FormatNumber(Fields!Total_Base_Imponible.Value*Fields!Tasa_del_Día.Value,2),
      IIf(Parameters!FiltroMoneda.Value="Dólar"And Fields!MonedaDocumento.Value="Bolívar",
      FormatNumber(Fields!Total_Base_Imponible.Value/Fields!Tasa_del_Día.Value,2),
      FormatNumber(Fields!Total_Base_Imponible.Value,2)
)
)</Value>
        </Field>
        <Field Name="TotalIvaAlCambio">
          <Value>=
IIf(Parameters!FiltroMoneda.Value="Bolívar"And Fields!MonedaDocumento.Value="Dólar",
      FormatNumber(Fields!Total_Iva.Value*Fields!Tasa_del_Día.Value,2),
      IIf(Parameters!FiltroMoneda.Value="Dólar"And Fields!MonedaDocumento.Value="Bolívar",
      FormatNumber(Fields!Total_Iva.Value/Fields!Tasa_del_Día.Value,2),
      FormatNumber(Fields!Total_Iva.Value,2)
)
)</Value>
        </Field>
        <Field Name="TotalDescuentoAlCambio">
          <Value>=
IIf(Parameters!FiltroMoneda.Value="Bolívar"And Fields!MonedaDocumento.Value="Dólar",
      FormatNumber(Fields!Total_Descuento.Value*Fields!Tasa_del_Día.Value,2),
      IIf(Parameters!FiltroMoneda.Value="Dólar"And Fields!MonedaDocumento.Value="Bolívar",
      FormatNumber(Fields!Total_Descuento.Value/Fields!Tasa_del_Día.Value,2),
      FormatNumber(Fields!Total_Descuento.Value,2)
)
)</Value>
        </Field>
        <Field Name="TotalFacturaAlCambio">
          <Value>=
IIf(Parameters!FiltroMoneda.Value="Bolívar"And Fields!MonedaDocumento.Value="Dólar",
      FormatNumber(Fields!Total_Factura.Value*Fields!Tasa_del_Día.Value,2),
      IIf(Parameters!FiltroMoneda.Value="Dólar"And Fields!MonedaDocumento.Value="Bolívar",
      FormatNumber(Fields!Total_Factura.Value/Fields!Tasa_del_Día.Value,2),
      FormatNumber(Fields!Total_Factura.Value,2)
)
)</Value>
        </Field>
      </Fields>
