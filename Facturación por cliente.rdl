 FORMAT(CONVERT(DATETIME, MAX(dbo.factura.Fecha), 102),'dd/MM/yyyy ') AS Fecha
 ,factura.Moneda AS [MonedaDocumento]
 ,factura.CodigoCliente
 ,Cliente.Nombre
 ,COUNT(factura.Numero) AS [Cantidad Facturas]

 ,SUM(dbo.factura.TotalRenglones) AS [Total Base Imponible]
 ,SUM(dbo.factura.TotalIVA) AS [Total Iva]
